{
    "name": "pucoder/apidoc",
    "description": "Laravel and lumen automatically generate online api documents \n Laravel和Lumen自动生成在线api文档",
    "minimum-stability": "stable",
    "keywords": ["document", "api", "laravel", "lumen", "apidoc"],
    "license": "MIT",
    "version": "v1.0.2",
    "type": "library",
    "authors": [
        {
            "name": "pudejun",
            "email": "xiaozhi2015@hotmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "laravel/lumen-framework": ">=6.0",
        "league/flysystem": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Pucoder\\Apidoc\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pucoder\\Apidoc\\ApiDocServiceProvider"
            ]
        }
    }
}
